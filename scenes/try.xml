<yas>
        <scene root="root" axis_length="4.0"/>

    <views default="cam-perspective-0">
        <perspective id="cam-perspective-0" near="0.5" far="500" angle="30">
            <from x="-25.0" y="15.0" z="10.0"/>
            <to x="60.0" y="-20.0" z="10.0"/>
        </perspective>
        <perspective id="cam-perspective-1" near="0.5" far="500" angle="30">
            <from x="100.0" y="10.0" z="30.0"/>
            <to x="0.0" y="0.0" z="10.0"/>
        </perspective>
        <perspective id="cam-perspective-2" near="0.5" far="500" angle="30">
            <from x="40.0" y="15.0" z="50.0"/>
            <to x="40.0" y="0.0" z="-10.0"/>
        </perspective>
        <ortho id="cam-ortho-1" near="0.5" far="500" left="3" right="7" top="5" bottom="6"/>
    </views>

    <ambient>
        <ambient r="1" g="1" b="1" a="1.0"/>
        <background r="0.5" g="0.74" b="0.94" a="1.0"/>
    </ambient>

    <lights>
        <omni id="omni-light-0" enabled="1">
            <location x="36" y="7" z="15" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="0.3" g="0.4" b="0.25" a="1.0"/>
            <specular r="0.9" g="0.8" b="0.7" a="1.0"/>
        </omni>
        <omni id="omni-light-1" enabled="1">
            <location x="23" y="-10" z="-10" w="1"/>
            <ambient r="0.05" g="0.10" b="0.15" a="1.0"/>
            <diffuse r="0.2" g="0.25" b="0.35" a="1.0"/>
            <specular r="0.6" g="0.7" b="1.0" a="1.0"/>
        </omni>
        <omni id="omni-light-2" enabled="1">
            <location x="0" y="5" z="8" w="1"/>
            <ambient r="0.05" g="0.05" b="0.05" a="1.0"/>
            <diffuse r="0.35" g="0.45" b="0.25" a="1.0"/>
            <specular r="0.8" g="0.8" b="0.8" a="1.0"/>
        </omni>
        <omni id="omni-light-3" enabled="1">
            <location x="33" y="10" z="-4" w="1"/>
            <ambient r="0.0" g="0.05" b="0.10" a="1.0"/>
            <diffuse r="0.25" g="1.0" b="1.0" a="1.0"/>
            <specular r="0.3" g="0.6" b="0.9" a="1.0"/>
        </omni>
        <spot id="spot-light-4" enabled="1" angle="30" exponent="15">
            <location x="45" y="6" z="10" w="1"/>
            <target x="45" y="0" z="10"/>
            <ambient r="0.03" g="0.05" b="0.01" a="1"/>
            <diffuse r="0.3" g="0.15" b="0.2" a="1"/>
            <specular r="0.7" g="0.8" b="0.6" a="1"/>
        </spot>
    </lights>

    <textures>
        <texture id="tex-bank" file="images/bank.jpg"/>
        <texture id="tex-leaves" file="images/leaves.jpg"/>
        <texture id="tex-rocks" file="images/rocks.jpg"/>
        <texture id="tex-trunk" file="images/trunk.jpg"/>
           <texture id="tex-snow" file="images/snow.jpg"/>
        <texture id="tex-vidral" file="images/vidral.jpg"/>
            <texture id="tex-carrot" file="images/carrot.jpg"/>
            <texture id="tex-eye" file="images/eye.jpg"/>
        <texture id="tex-wood" file="images/wood.jpg"/>

    </textures>

    <materials>
        <material id="mat-1" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="0"/>
            <ambient r="0.2" g="0.2" b="0.3" a="1"/>
            <diffuse r="0.6" g="0.6" b="0.5" a="1"/>
            <specular r="0.4" g="0.7" b="0.8" a="1"/>
        </material>
    </materials>
 
    <transformations>
        <transformation id="show-0">
            <translate x="0" y="0" z="0"/>
            <rotate axis="x" angle="-90"/>
        </transformation>
       <transformation id="tree">
        <translate x="5" y="0" z="2"/>
        <scale x="1.5" y="3" z="1.5"/>
        </transformation>
        <transformation id="basic-tanslation">
        <translate x="2" y="0" z="1"/>
    <rotate axis="y" angle="-90"/>
        </transformation>

    </transformations>
    

    <primitives>
        <primitive id="rect">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5"/>
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" stacks="32" slices="32"/>
        </primitive>
        <primitive id="cilindro">
            <cylinder base="1" top="1" height="1" stacks="32" slices="32"/>
        </primitive>
        
        <primitive id="cone">
            <cylinder base="1" top="0" height="1" stacks="32" slices="32"/>
        </primitive>
        <primitive id="trian">
        <triangle x1="-0.5" y1="-0.5" z1="0" x2="0" y2="0.5" z2="0" x3="0.5" y3="-0.5" z3="0"/>
        </primitive>
    </primitives>

    <components>
    
        <component id="root">
            <transformation>
                <translate x="0" y="0" z="0"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
              <!--  <componentref id="cube"/>-->
                <componentref id="christmas-tree"/>
                <componentref id="snow-doll"/>
          
            </children>
        </component>

        <component id="cube-lado1">
            <transformation>
                <translate x="0" y="0" z="-0.5"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>
            </children>
        </component>

       <component id="cube-lado2">
            <transformation>
                <translate x="0" y="0" z="0.5"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>

            </children>
        </component>

        <component id="cube-lado3">
            <transformation>
                <translate x="0" y="-0.5" z="0"/>
                <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>

            </children>
        </component>


        <component id="cube-lado4">
            <transformation>
                <translate x="0" y="0.5" z="0"/>
                <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>

            </children>
        </component>

         <component id="cube-lado5">
            <transformation>
                <translate x="-0.5" y="0" z="0"/>
                <rotate axis="y" angle="90"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>

            </children>
        </component>

         <component id="cube-lado6">
            <transformation>
                <translate x="0.5" y="0" z="0"/>
                <rotate axis="y" angle="90"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <primitiveref id="rect"/>
            </children>
        </component>
        <component id="cube">
            <transformation>
                <translate x="0" y="0" z="0"/>
                <rotate axis="y" angle="90"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-rocks" length_s="3.0" length_t="3.0"/>
            <children>
                <componentref id="cube-lado1"/>
                <componentref id="cube-lado2"/>
                <componentref id="cube-lado3"/>
                <componentref id="cube-lado4"/>
                <componentref id="cube-lado5"/>
                <componentref id="cube-lado6"/>
            </children>
        </component>

<!-- Christmas Tree-->
        <component id="christmas-tree">
            <transformation> 
                <transformationref id="tree"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-vidral" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="cone"/>
                <primitiveref id="trian"/>
            </children>
        </component>

<!-- snow doll-->

   <component id="snow-doll">
            <transformation> 
                <transformationref id="basic-tanslation"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-snow" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="sphere"/>
                <componentref id="midle-ball"/>
                <componentref id="last-ball"/>
                <componentref id="nose"/>
                <componentref id="eye1"/>
                <componentref id="eye2"/>
                <componentref id="arm"/>
                <componentref id="arm2"/>


            </children>
        </component>
  
   <component id="midle-ball">
            <transformation> 
                <translate x="0" y="1.5" z="0"/>
                  <scale x="0.7" y="0.7" z="0.7"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-snow" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>


   <component id="last-ball">
            <transformation> 
                <translate x="0" y="2.6" z="0"/>
                  <scale x="0.5" y="0.5" z="0.5"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-snow" length_s="4.0" length_t="4.0"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>


    <component id="nose">
            <transformation> 

                <translate x="0" y="2.6" z="0.5"/>
              <rotate axis="x" angle="90"/>
                  <scale x="0.1" y="0.3" z="0.1"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-carrot" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="cone"/>
            </children>
        </component>


    <component id="eye1">
            <transformation> 
                <translate x="-0.2" y="2.8" z="0.45"/>

                  <scale x="0.07" y="0.07" z="0.025"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-eye" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>

 <component id="eye2">
            <transformation> 
                <translate x="0.2" y="2.8" z="0.45"/>

                  <scale x="0.07" y="0.07" z="0.025"/>
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-eye" length_s="1.0" length_t="1.0"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>

 <component id="arm">
            <transformation> 
            <translate x="0.5" y="1.7" z="0.2"/>
            <rotate axis="z" angle="50"/>
             <scale x="1.25" y="0.025" z="0.025"/>
     
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-wood" length_s="1.0" length_t="1.0"/>
            <children>
                <componentref id="cube"/>
            </children>
        </component>

 <component id="arm2">
            <transformation> 
            <translate x="-0.5" y="1.7" z="0.2"/>
            <rotate axis="z" angle="-50"/>
             <scale x="1.25" y="0.025" z="0.025"/>
     
            </transformation>
            <materials>
                <material id="mat-1"/>
            </materials>
            <texture id="tex-wood" length_s="1.0" length_t="1.0"/>
            <children>
                <componentref id="cube"/>
            </children>
        </component>





    </components>
    


</yas>
