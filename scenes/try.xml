<yas>
    <scene root="root" axis_length="4.0"/>

    <views default="cam-perspective-0">
        <perspective id="cam-perspective-0" near="0.5" far="500" angle="30">
            <from x="-15.0" y="15.0" z="15.0"/>
            <to x="0.0" y="0.0" z="0.0"/>
        </perspective>
        <perspective id="cam-perspective-1" near="0.5" far="500" angle="30">
            <from x="100.0" y="10.0" z="30.0"/>
            <to x="0.0" y="0.0" z="10.0"/>
        </perspective>
        <perspective id="cam-perspective-2" near="0.5" far="500" angle="30">
            <from x="40.0" y="15.0" z="50.0"/>
            <to x="40.0" y="0.0" z="-10.0"/>
        </perspective>
        <ortho id="cam-ortho-1" near="0.5" far="500" left="3" right="7" top="5" bottom="6"/>
    </views>

    <ambient>
        <ambient r="1" g="1" b="1" a="1.0"/>
        <background r="0.5" g="0.74" b="0.94" a="1.0"/>
