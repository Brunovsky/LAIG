<yas>
    <scene axis_length="50.0" root="root"/>

    <views default="origin">
        <perspective id="origin" angle="50" near="0.5" far="500">
            <from x="25.0" y="15.0" z="25.0"/>
            <to x="0.0" y="-3.0" z="0.0"/>
